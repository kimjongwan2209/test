const mongoose = require("mongoose");

const postSchema = new mongoose.Schema({
  postId:{
    type: String,
    required: true,
  },
  password:{
    type:String,
    required: true,
  },
  title:{
    type: String,
  },
  contenet:{
    type: String,
  },
  createdAt:{
    type:Date,
    default:Date.now
  }
}) 
