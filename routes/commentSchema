const mongoose = require("mongoose");

const commentSchema = new mongoose.Schema({
  commentId:{
    type: String,
    required: true,
  },
  password:{
    type: String,
    required: true,
  },
  content:{
    type: String,
  },
  createdAt:{
    type:Date,
    default: Date.now
  }
 
}) 


